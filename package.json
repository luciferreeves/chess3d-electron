{
  "name": "chess3d",
    "productName": "Chess3D",
  "appCopyright": "BackSlash Linux Apps LLC.",
  "appCategoryType": "Games",
  "version": "1.0.0",
  "description": "Intteractive 3D Chess Game",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
      "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=icons/mac.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . chess3d --overwrite --asar=true --platform=win32 --arch=ia32 --icon=icons/windows.ico --prune=true --out=release-builds --version-string.CompanyName=BackSlashLinux --version-string.FileDescription=CE --version-string.ProductName=\"Chess3D\"",    
    "package-linux": "electron-packager . chess3d --overwrite --asar=true --platform=linux --arch=x64 --icon=icons/icon.png --prune=true --out=release-builds"   
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "2.0.2",
    "electron-packager": "^12.1.0"
  }
}
